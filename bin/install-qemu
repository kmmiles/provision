#!/usr/bin/env bash

#shellcheck source=../common.profile
source "$(cd "$(dirname "${BASH_SOURCE[0]}")/.." && pwd)/common.profile"
require_non_root

if ! chkpath qemu-system-x86_64 qemu-img kvm-ok; then
  sudo apt --no-install-recommends -y install \
    cpu-checker \
    qemu-kvm \
    qemu-utils
fi

