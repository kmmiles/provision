#!/usr/bin/env bash

#shellcheck source=../common.profile
source "$(cd "$(dirname "${BASH_SOURCE[0]}")/.." && pwd)/common.profile"
require_non_root
require_wsl

wslconfig="$HOME/winhome/.wslconfig"
if [ ! -f "$wslconfig" ]; then
  echo "Nothing to do."
  exit 0
fi

filename=".wslconfig-$(date "+%s")"
wslconfig_new="$HOME/winhome/$filename"
status_msg "Renaming .wslconfig to $filename..."
mv -f "$wslconfig" "$wslconfig_new"
status_ok
